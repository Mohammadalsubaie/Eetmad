{
  "name": "eetmad",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{ts,tsx,json,md,css}\"",
    "type-check": "tsc --noEmit",
    "validate-naming": "tsx scripts/src/validation/validate-naming.ts",
    "validate-types": "tsx scripts/src/validation/validate-types.ts",
    "validate:design": "tsx scripts/src/validation/validate-design-rules.ts",
    "validate:translations": "tsx scripts/src/validation/validate-translations.ts",
    "check:translations": "tsx scripts/src/monitoring/check-runtime-errors.ts",
    "watch:translations": "tsx scripts/src/monitoring/watch-dev-server.ts",
    "validate:all": "npm run type-check && npm run lint && npm run validate:design src/",
    "validate:watch": "nodemon --watch src/ --ext tsx,ts --exec 'npm run validate:design src/'",
    "scripts:menu": "node scripts/tools/menu.js",
    "scripts:help": "cat scripts/INDEX.md",
    "check:health": "npm run type-check && npm run lint:check && npm run format:check && npm run validate:design src/",
    "setup:hooks": "chmod +x scripts/src/utils/setup-hooks.sh && scripts/src/utils/setup-hooks.sh",
    "analyze:structure": "tsx scripts/src/analysis/analyze-structure.ts",
    "analyze:structure:v2": "tsx scripts/src/analysis/analyze-structure-v2.ts",
    "add:breadcrumbs": "tsx scripts/src/analysis/add-breadcrumbs.ts",
    "fix:breadcrumbs": "tsx scripts/src/analysis/fix-breadcrumb-translations.ts",
    "detect:routes": "tsx scripts/src/analysis/detect-routes.ts",
    "prepare": "husky",
    "lint:check": "eslint . --ext .ts,.tsx",
    "format:check": "prettier --check \"**/*.{ts,tsx,json,md,css}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "check:all": "npm run type-check && npm run lint:check && npm run format:check",
    "check:unused": "npx ts-unused-exports tsconfig.json",
    "check:circular": "npx madge --circular --extensions ts,tsx src"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.0",
    "axios": "^1.6.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.0",
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.553.0",
    "next": "^16.0.1",
    "next-intl": "^4.5.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.51.0",
    "react-router-dom": "^7.9.5",
    "recharts": "^3.4.1",
    "tailwind-merge": "^2.2.0",
    "zod": "^3.22.0",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "autoprefixer": "^10.4.16",
    "esbuild": "^0.27.0",
    "eslint": "^9.39.1",
    "eslint-config-next": "^16.0.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-unused-imports": "^4.3.0",
    "glob": "^12.0.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "lint-staged": "^16.2.6",
    "madge": "^8.0.0",
    "msw": "^2.2.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.10",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "ts-unused-exports": "^11.0.1",
    "tsx": "^4.20.6",
    "typescript": "^5"
  },
  "optionalDependencies": {
    "@esbuild/darwin-x64": "^0.27.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "npm run validate:design"
    ],
    "*.{json,md,css}": [
      "prettier --write"
    ]
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
